{
  "name": "zenlend-frontend",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@starknet-io/get-starknet": "^4.0.8",
    "chart.js": "^4.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently \"npm:backend\" \"npm:frontend\"",
    "frontend": "react-scripts start",
    "backend": "cd ../commitments && python app.py",
    "install-backend": "cd ../commitments && pip install -r requirements.txt",
    "clean": "rm -rf build node_modules",
    "deploy": "npm run build && echo 'Deploy build folder to your hosting service'",
    "analyze": "npm run build && npx serve -s build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "concurrently": "^7.6.0"
  }
}
